{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node@3.0.17"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ],
  "env": {
    "FIREBASE_ADMIN_CREDENTIALS_JSON": "firebase_admin_credentials_json",
    "VERCEL_URL": "vercel_url"
  }
}
